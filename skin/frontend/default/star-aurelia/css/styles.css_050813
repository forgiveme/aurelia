/*Cart gift wrap and message styling*/
body.checkout-cart-index button span {padding:3px 2px;text-transform:uppercase;}
.cart-right-col { width:35%; float:right;}
.cart .cart-collaterals div.col2-set {  width: 60%;}
.cart div.totals {width: 35%;}
.cart .cart-collaterals .col2-set .col-2 { width:100%}
.col2-set div.col-1 { display:none;}
div.giftwrap { width:100%}
.giftwrap_header { float:left; width:100%;  }
.giftwrap_header h2 { color: #BAB6B2;  margin-bottom: 0;}
.gift-holder {float:left; width:100%;   margin-top: 10px;}
.gift-message-fields {float:left; width:100%;}
.gift-message-fields label { padding-bottom: 5px;  padding-top: 5px;float:left; display:block; clear:both;}
.gift-message-fields input { float:left; display:block; clear:both;}
.gift-message-fields textarea { float:left; display:block; clear:both;  height: 113px !important;width: 254px !important;}
.gift-message-fields .gift-submit {margin-top: 10px; padding:3px 2px; float:left; display:block; clear:both;}
.cart .discount { width:89%;  margin-bottom: 10px;}
.cart .discount label { font-size:1.2em;}
.cart div.totals { width:99%;}
.gift-submit {background-color: #95918E; text-transform: uppercase;  background-image: none;  border: 1px solid #7E7F82;  color: #FFFFFF;  cursor: pointer;  font-family: 'Vollkorn',serif;
    font-size: 1.1em;   letter-spacing: 0.1em;}
	
/*Unison Gift buttons*/
div.checker {    margin-right: 10px;}
div.checker, div.checker span, div.checker input {    height: 23px;    width: 23px;}
div.checker {    position: relative;}

div.selector, div.checker, div.button, div.radio, div.uploader {    display: inline-block;    vertical-align: middle;}

div.checker span {  background-position: -5px -29px;    display: inline-block;    text-align: center;}
div.checker span.checked {    background-position: -5px 0px;}
div.selector, div.selector span, div.checker span, div.radio span, div.uploader, div.uploader span.action, div.button, div.button span {    background-image: url("../images/gift_message_check.png");    background-repeat: no-repeat;}

div.selector, div.selector *, div.radio, div.radio *, div.checker, div.checker *, div.uploader, div.uploader *, div.button, div.button * {    margin: 0;    padding: 0;}
div.checker input {    background: none repeat scroll 0 0 transparent;    border: medium none;    display: inline-block;    opacity: 0;}
div.checker input, input[type="search"], input[type="search"]:active {  -moz-appearance: none;}

